<style>
  .header_5_wrapper .main-header{
    background-color:{{section.settings.main_bgheader }};
  }
  .header_5 .myaccount .dropdown-toggle .account-title,
  .header_5 .site-header__search .serach-title{
    color:{{section.settings.Header_link_color }};
  }
  .header_5 .site-header__cart-count .cart-products-count{
    color:{{section.settings.count_color }};
  }
  .site-header__search .serach_icon svg,.header_5 .right-link-icon .myaccount svg,.header_5 .wrapper-top-cart > .site-header_cart_link svg{
    fill:{{section.settings.social_icon_color }};
  }
  .header_5 .right-link-icon .site-header__search.icon-btn .serach_icon:hover svg,.header_5 .right-link-icon .myaccount:hover svg,.header_5 .myaccount .dropdown-toggle.open .account-title, .header_5 .wrapper-top-cart > .site-header_cart_link:hover svg{
    fill:{{section.settings.top_header_text_hover_color}};
  }
  .header_5 #tt-megamenu1 .tt-mega_menu1 li.tt_menu_item a,.header_5 #tt-megamenu1 .tt-mega_menu1 li.tt_menu_item i{
    color:{{section.settings.vartical_txt_menu }};
  }
  .tt-megamenu1{
    background:{{section.settings.vartical_bg_menu }};
  }
 .header_5 #tt-megamenu .tt_sub_menu_wrap .tt_sub_menu h4.sub_mn_title a{
    color:{{section.settings.sub_txt_header }};
  }
  .header_5 .top-header,
  .top_header_5 .header_language .disclosure .language_wrapper, 
  .top_header_5 .currency_wrapper.dropdown-toggle,  
  .top_header_5 .header-email{color:{{section.settings.top_header_text_color }};}    

  .header_5 .top_header_5{ background-color:{{section.settings.top_bg_color }};}
  @media (min-width: 992px)
  {
    .top-header .social-block .social-icon svg,
    .top_header_5 .header-email svg{
      fill:{{section.settings.social_icon_color }};
    }
    .header_5_wrapper #tt-megamenu .tt-mega_menu li.tt_menu_item a,.header_5 .right-link-icon .myaccount:hover, .header_5 .site-header__search .serach_icon:hover .serach-title{
      color:{{section.settings.main_txt_header }};
    }
    .header_5 .offer-content:hover svg{fill:{{section.settings.offer_text_hover_color}};}
    .header_5 .offer-content{color:{{section.settings.offer_text_color }};}
    .header_5 .offer-content:hover{
      color:{{section.settings.offer_text_hover_color }};
    }
    .header_5_wrapper .tt-megamenu .tt_menus_ul > li .menu_detail .img-icon{
      background:{{section.settings.img_icon_color}};
    }
    .header_5_wrapper .main-header .fullscreen_header_toggle{
      background:{{section.settings.tt-title_bgclr }};	
    }
    .header_5 .offer-content svg,.header_5 .header-service .header-contect svg{
      fill:{{section.settings.offer_text_color }};
    }
    .header_5_wrapper .main-header .fullscreen_header_toggle:hover,.header_5_wrapper .main-header .fullscreen_header_toggle.active,.header_5_wrapper .main-header .fullscreen_header_toggle:focus{
      background:{{section.settings.tt-title_hov_bgclr }};	
    }
    .header_5 .fullscreen_header_toggle .leftmenu span.toggle_text,.header_5_wrapper .main-header .fullscreen_header_toggle::after{
      color:{{section.settings.tt-title_clr }};	
    }
    .header_5 .fullscreen_header_toggle:hover .leftmenu span.toggle_text{
      color:{{section.settings.tt-title_hov_clr }};	
    }
    .header_5_wrapper .main-header .fullscreen_header_toggle .leftmenu #menu,
    .header_5_wrapper .main-header .fullscreen_header_toggle .leftmenu #menu::before,
    .header_5_wrapper .main-header .fullscreen_header_toggle .leftmenu #menu::after{
      background:{{section.settings.tt-title_clr }};
    }
    .header_5 .header-service .header-contect{
      color:{{section.settings.top_header_text_color }};
    }
    .header_5 .right-link-icon .myaccount .dropdown-toggle:hover .account-title,.header_5 .right-link-icon .myaccount .dropdown-toggle.open .account-title,
    .header_5 .right-link-icon .myaccount .dropdown-toggle:hover .mdi.mdi-chevron-down,.header_5 .right-link-icon .myaccount.open .mdi.mdi-chevron-up,.header_5 .right-link-icon .site-header__search:hover .serach_icon{
      color:{{section.settings.top_header_text_hover_color }};
    }
    .header_5_wrapper #tt-megamenu .tt-mega_menu li.tt_menu_item:hover>a{
      color:{{section.settings.menu_txt_hvrcolor }};
    }
    .header_5 .header-service .header-contect .contact-info{
      color:{{section.settings.contact_color}};
    }
  }
</style>
{% capture navSize %}250px{% endcapture %}
{% capture navHeaderSize %}{{ section.settings.menu_mobile }}px{% endcapture %}
{% capture logoMaxHeight %}{{ section.settings.menu_mobile | minus: 30 }}px{% endcapture %}
{% capture borderWidth %}{{ settings.border_width }}px{% endcapture %}
{% capture colorBorder %}{{ settings.color_borders }}{% endcapture %}
{% assign navMenu = linklists[section.settings.nav_menu] %}
{%- assign sticky_class = '' -%}
{% if section.settings.sticky_header %}{%- assign sticky_class = 'sticky_header' -%}{% endif %}
